    "@endo/init": "^0.5.56",
    "@endo/marshal": "^0.8.5",
    "@endo/nat": "^4.1.27",
    "@endo/promise-kit": "^0.2.56",
    "@endo/ses-ava": "^0.2.40",
    "@endo/zip": "^0.2.31",
    "ansi-styles": "^6.2.1",
    "anylogger": "^0.21.0",
    "import-meta-resolve": "^2.2.1",
    "microtime": "^3.1.0",
    "semver": "^6.3.0",
    "tmp": "^0.2.1"
  },
  "peerDependencies": {
    "ava": "^5.3.0"
