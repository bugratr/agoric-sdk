    "yargs-parser": "^21.1.1"
  },
  "dependencies": {
    "@agoric/assert": "^0.6.0",
    "@agoric/internal": "^0.3.2",
    "@agoric/store": "^0.9.2",
    "@agoric/swing-store": "^0.9.1",
    "@agoric/swingset-liveslots": "^0.10.2",
    "@agoric/swingset-xsnap-supervisor": "^0.10.2",
    "@agoric/time": "^0.3.2",
    "@agoric/vat-data": "^0.5.2",
    "@agoric/xsnap": "^0.14.2",
    "@agoric/xsnap-lockdown": "^0.14.0",
