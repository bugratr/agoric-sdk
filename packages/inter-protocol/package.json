{
  "name": "@agoric/inter-protocol",
  "version": "0.14.0",
  "description": "Core cryptoeconomy contracts",
  "type": "module",
  "main": "src/index.js",
  "engines": {
    "node": ">=14.15.0"
  },
  "scripts": {
    "build": "yarn build:bundles",
    "build:bundles": "node ./scripts/build-bundles.js",
    "test": "ava",
    "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix",
    "lint": "run-s --continue-on-error lint:*",
    "lint:eslint": "eslint .",
    "lint:types": "tsc -p jsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Agoric/agoric-sdk.git"
  },
  "author": "Agoric",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Agoric/agoric-sdk/issues"
  },
  "homepage": "https://github.com/Agoric/agoric-sdk#readme",
  "dependencies": {
    "@agoric/assert": "^0.6.0",
    "@agoric/ertp": "^0.16.0",
    "@agoric/governance": "^0.10.0",
    "@agoric/internal": "^0.3.0",
    "@agoric/notifier": "^0.6.0",
    "@agoric/store": "^0.9.0",
    "@agoric/time": "^0.3.0",
    "@agoric/vat-data": "^0.5.0",
    "@agoric/vats": "^0.14.0",
    "@agoric/zoe": "^0.26.0",
    "@endo/captp": "^3.1.1",
    "@endo/eventual-send": "^0.17.2",
    "@endo/far": "^0.2.18",
    "@endo/marshal": "^0.8.5",
    "@endo/nat": "^4.1.27",
    "jessie.js": "^0.3.2"
  },
  "devDependencies": {
    "@agoric/deploy-script-support": "^0.10.0",
    "@agoric/smart-wallet": "^0.5.0",
    "@agoric/swingset-liveslots": "^0.10.0",
    "@agoric/swingset-vat": "^0.32.0",
    "@endo/bundle-source": "^2.5.1",
    "@endo/init": "^0.5.56",
    "@endo/promise-kit": "^0.2.56",
    "@fast-check/ava": "^1.1.3",
    "ava": "^5.2.0",
    "c8": "^7.13.0",
    "deep-object-diff": "^1.1.9",
    "import-meta-resolve": "^2.2.1"
  },
  "files": [
    "bundles/",
    "scripts/",
    "src/",
    "exported.js",
    "NEWS.md"
  ],
  "ava": {
    "files": [
      "test/**/test-*.js"
    ],
    "concurrency": 2,
    "workerThreads": false,
    "timeout": "10m"
  },
  "publishConfig": {
    "access": "public"
  }
}
